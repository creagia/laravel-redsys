{
    "name": "creagia/laravel-redsys",
    "description": "Laravel Redsys Payments Gateway",
    "keywords": [
        "creagia",
        "laravel",
        "php",
        "payment gateway",
        "redsys"
    ],
    "homepage": "https://github.com/creagia/laravel-redsys",
    "license": "MIT",
    "authors": [
        {
            "name": "David Torras",
            "email": "david@creagia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "creagia/redsys-php": "^3.0.1",
        "illuminate/contracts": "^8.73|^9.0|^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "laravel/pint": "^1.6",
        "nunomaduro/collision": "^5.10|^6.0|^8.0",
        "larastan/larastan": "^1.0|^2.0|^3.0",
        "orchestra/testbench": "^6.23|^7.0|^8.0|^9.0|^10.0",
        "pestphp/pest": "^1.21|^2.34|^3.7",
        "pestphp/pest-plugin-laravel": "^1.1|^2.3|^3.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "phpstan/phpstan-phpunit": "^1.0|^2.0",
        "phpunit/phpunit": "^9.5|^10.5|^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "Creagia\\LaravelRedsys\\": "src",
            "Creagia\\LaravelRedsys\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creagia\\LaravelRedsys\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creagia\\LaravelRedsys\\LaravelRedsysServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
